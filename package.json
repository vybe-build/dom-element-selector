{
  "name": "@vybe-adk/dom-element-selector",
  "version": "0.1.0",
  "description": "A React hook for visually selecting DOM elements on a page with an interactive overlay",
  "license": "MIT",
  "author": "Vybe",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vybe-build/dom-element-selector.git"
  },
  "bugs": {
    "url": "https://github.com/vybe-build/dom-element-selector/issues"
  },
  "homepage": "https://github.com/vybe-build/dom-element-selector#readme",
  "keywords": [
    "react",
    "dom",
    "element",
    "selector",
    "picker",
    "inspector",
    "overlay",
    "hook"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^28.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}